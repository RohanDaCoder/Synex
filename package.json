{
	"name": "synex",
	"version": "1.0.0",
	"description": "A feature-rich Discord bot built with TypeScript and Discord.js v14.",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"format": "prettier -w .",
		"build": "tsc && tsup --config tsup.config.ts",
		"start": "bun dist/index.js",
		"lint": "eslint 'src/**/*.ts'",
		"deploy": "bun run lint && bun run build && bun run start"
	},
	"keywords": [
		"discord",
		"bot",
		"typescript",
		"discord.js"
	],
	"author": "RohanDaCoder",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.7.7",
		"calm.db": "^1.1.5",
		"colors": "^1.4.0",
		"discord.js": "^14.16.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"eslint": "^9.15.0",
		"globals": "^15.12.0",
		"prettier": "^3.3.3",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.14.0"
	}
}
