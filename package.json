{
	"name": "synex",
	"version": "1.0.0",
	"description": "A feature-rich Discord bot built with TypeScript and Discord.js v14.",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"format": "prettier -w .",
		"build": "bun build src/index.ts --target=bun --sourcemap --outdir=dist/ --minify",
		"start": "bun run dist/index.js",
		"deploy": "bun run build && bun run start"
	},
	"keywords": [
		"discord",
		"bot",
		"typescript",
		"discord.js"
	],
	"author": "RohanDaCoder",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.11.0",
		"calm.db": "^1.1.5",
		"colors": "^1.4.0",
		"discord.js": "^14.22.1",
		"dotenv": "^17.2.2",
		"parse-duration": "^2.1.4"
	},
	"devDependencies": {
		"prettier": "^3.6.2"
	}
}
