{
	"name": "synex",
	"module": "index.ts",
	"private": true,
	"scripts": {
		"start": "bun dist/index.js",
		"dev": "bun --watch run src/",
		"build": "bun run lint && tsc --noEmit && bun build src/index.ts --sourcemap --minify --outdir=dist/ --target=bun",
		"build:node": "bun run build --target=node",
		"deploy": "bun run build && bun run start",
		"lint": "oxlint -f stylish",
		"format": "prettier -w ."
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.13.2",
		"discord.js": "^14.25.1",
		"dotenv": "^17.2.3",
		"parse-duration": "^2.1.4",
		"yoctocolors": "^2.1.2"
	},
	"devDependencies": {
		"oxlint": "^1.32.0",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3"
	}
}
