{
	"name": "synex",
	"module": "index.ts",
	"private": true,
	"scripts": {
		"start": "bun dist/index.js",
		"dev": "bun --watch run src/",
		"build": "bun build src/index.ts --sourcemap --minify --outdir=dist/ --target=bun",
		"deploy": "bun run build && bun run start",
		"format": "prettier -w ."
	},
	"type": "module",
	"dependencies": {
		"discord.js": "^14.25.1",
		"dotenv": "^17.2.3",
		"yoctocolors": "^2.1.2"
	},
	"devDependencies": {
		"prettier": "^3.6.2"
	}
}
